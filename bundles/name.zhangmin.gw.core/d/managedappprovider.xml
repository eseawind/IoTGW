<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" immediate="true" name="name.zhangmin.gw.core.apps.managedappprovider">
   <implementation class="name.zhangmin.gw.core.apps.ManagedAppProvider"/>
   
   <service>
      <provide interface="name.zhangmin.gw.core.apps.AppProvider"/>
      <provide interface="name.zhangmin.gw.core.apps.ManagedAppProvider"/>
   </service>
   
   <reference bind="addStorageService" cardinality="1..n" 
      interface="name.zhangmin.gw.core.storage.StorageService" name="StorageService" 
      policy="dynamic" unbind="removeStorageService"/>
   <reference bind="addAppFactory" cardinality="0..n" 
      interface="name.zhangmin.gw.core.apps.AppFactory" name="AppFactory" 
      policy="dynamic" unbind="removeAppFactory"/>
</scr:component>
